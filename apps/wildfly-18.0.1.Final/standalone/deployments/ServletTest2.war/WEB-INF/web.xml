<web-app>
	<display-name>Archetype Created Web Application</display-name>
  
	<servlet>
		<servlet-name>GetBalanceServlet</servlet-name>
		<display-name>GetBalanceServlet</display-name>
		<description></description>
		<servlet-class>controller.GetBalanceServlet</servlet-class>
		<init-param>
			<param-name>GetBalanceGreeting</param-name>
			<param-value>How can we serve you?</param-value>
			<description></description>
		</init-param>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>GetBalanceServlet</servlet-name>
		<url-pattern>/balance</url-pattern>
	</servlet-mapping>
 
</web-app>
